{
  "manifest_version": 2,
  "name": "__MSG_name__",
  "version": "3.5.0",
  "default_locale": "en",
  "description": "__MSG_description__",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "assets/js/jquery/jquery.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "Visual Style Editor"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "*://*/*"
  ],
  "web_accessible_resources": [
    /*"font/fontawesome-webfont.ttf",
    "font/fontawesome-webfont.woff",
    "128.png",*/
    "plugin.html",
    "assets/**/*"
  ]
}
